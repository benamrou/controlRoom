SELECT 'PRE-CHECK' "STEP",
       CNTFILE "FILE",
       CNTLGFI "LINE",
       CNTLOC "SITE",
       CNTCODE1 "ITEM",
       CNTUPC "UPC",
       CNTDESC "ITEMDESC",
       CNTQTY "QTY",
       CNTTRT "TRT",
       CNTNERR "NERR",
       CNTMESS "MESS"
  FROM THIRDPARTY_COUNTING@HEINENS_CUSTOM_PROD
 WHERE CNTTRT IN (2, 3)
   AND TRUNC(CNTDCRE) = TO_DATE('04/29/2019', 'MM/DD/RRRR')
   --AND CNTLOC = '41'
   -- AND CNTFILE LIKE '000041.txt%'
UNION
SELECT 'GOLD-CHECK' "STEP",
       CNTFILE "FILE",
       CNTLGFI "LINE",
       CNTLOC "SITE",
       CNTUPC "UPC",
       CNTCODE1 "ITEM",
       CNTDESC "ITEMDESC",
       CNTQTY "QTY",
       IVFTRT "TRT",
       IVFNERR "NERR",
       IVFMESS "MESS"
  FROM THIRDPARTY_COUNTING@HEINENS_CUSTOM_PROD, INTINV@HEINENS_CUSTOM_PROD
 WHERE IVFTRT = 2
   AND IVFFICH = CNTFILE
   AND IVFLGFI = CNTLGFI
   --AND IVFFICH LIKE '000041.txt%'
   AND TRUNC(IVFDINV) = TO_DATE('04/29/2019', 'MM/DD/RRRR')
   --AND IVFSITE = 41
 ORDER BY "SITE" ASC, LINE, "TRT", "NERR" ASC
